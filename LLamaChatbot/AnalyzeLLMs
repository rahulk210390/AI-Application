# write a function to sum two number

def add_two_numbers(num1, num2): 
    return num1 + num2          


print(add_two_numbers(4,6))


# write a function to divide two numbers

def div_two_numbers(num1, num2): 			
    if num2 == 0: 				
        return "Division by zerois not possible!" 		
    else: 			
        return num1 / num2











